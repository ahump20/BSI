  "main": "index-enhanced-full.html",
  "scripts": {
    "start": "python3 main.py",
    "dev": "python3 -m http.server 8000",
    "serve": "python3 -m http.server 8000",
    "build": "echo 'Build complete - BSI platform ready'",
    "deploy": "./deploy-blazesportsintel.sh",
    "deploy:staging": "wrangler pages deploy --env staging",
    "deploy:production": "wrangler pages deploy --env production",
    "test": "python3 -m pytest test_biomech.py -v",
    "biomech": "python3 pose.py",
    "monte-carlo": "python3 monte_carlo.py",
    "vision": "python3 main.py --vision-mode",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "format": "black *.py && prettier --write *.html *.js",
    "lint": "flake8 *.py && eslint *.js",
    "install": "pip install -r requirements.txt"
  },
  "keywords": [
    "sports-analytics",
    "biomechanics",
    "computer-vision",
    "3d-pose",
    "championship-intelligence",
    "deep-south-sports",
