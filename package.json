{
  "name": "blaze-sports-intel-bsi",
  "version": "2.1.0",
  "description": "ðŸ”¥ Blaze Sports Intel - Biomechanics Vision System & Championship Intelligence Platform",
  "main": "index-enhanced-full.html",
  "type": "module",
  "scripts": {
    "start": "python3 main.py",
    "dev": "wrangler pages dev . --port=8787 --local --live-reload",
    "dev:functions": "wrangler pages dev . --port=8787 --local",
    "dev:python": "python3 -m http.server 8000",
    "dev:all": "concurrently \"npm:dev:functions\" \"npm:build:watch\"",
    "serve": "python3 -m http.server 8000",
    "build": "npm run build:ts && npm run tokens:build && npm run build:verify",
    "build:ts": "tsc --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json --watch",
    "build:verify": "node scripts/verify-build.js",
    "clean": "rm -rf dist && rm -rf lib/**/*.js lib/**/*.d.ts lib/**/*.js.map && rm -rf .wrangler",
    "prebuild": "npm run clean",
    "deploy": "./deploy.sh",
    "deploy:staging": "wrangler pages deploy . --project-name blazesportsintel --branch staging",
    "deploy:production": "wrangler pages deploy . --project-name blazesportsintel --branch main",
    "deploy:preview": "wrangler pages deploy . --project-name blazesportsintel --branch preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,html,md}\" && black *.py",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,html,md}\"",
    "lint": "eslint \"**/*.{js,ts,jsx,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"**/*.{js,ts,jsx,tsx}\" --fix",
    "lint:all": "npm run lint && flake8 *.py",
    "tokens:build": "style-dictionary build --config tokens/config.js",
    "tokens:watch": "chokidar 'tokens/**/*.json' -c 'npm run tokens:build'",
    "prepare": "husky install",
    "biomech": "python3 pose.py",
    "monte-carlo": "python3 monte_carlo.py",
    "vision": "python3 main.py --vision-mode",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "setup:db": "node scripts/setup-database.js",
    "setup:all": "npm run setup:db && npm run train:models",
    "ml:train": "node scripts/train-models.js",
    "ml:predict": "node scripts/run-predictions.js",
    "ml:health": "node scripts/check-ml-health.js",
    "api:start": "node api/server.js",
    "api:dev": "node --watch api/server.js",
    "api:test": "vitest run tests/api",
    "health": "node scripts/health-check.js",
    "migrate": "node scripts/run-migrations.js",
    "seed": "node scripts/seed-data.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "wrangler:login": "wrangler login",
    "wrangler:whoami": "wrangler whoami",
    "wrangler:d1:execute": "wrangler d1 execute blazesports-db --remote",
    "wrangler:kv:list": "wrangler kv key list --namespace-id=a53c3726fc3044be82e79d2d1e371d26 --remote",
    "wrangler:pages:list": "wrangler pages deployment list --project-name blazesportsintel",
    "wrangler:tail": "wrangler pages deployment tail --project-name blazesportsintel",
    "lighthouse": "lighthouse https://blazesportsintel.com --view --output=html --output-path=./lighthouse-report.html",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:deps": "npx npm-check-updates",
    "docs:generate": "npm run generate:charts && npm run generate:stackoverflow"
  },
  "keywords": [
    "sports-analytics",
    "biomechanics",
    "computer-vision",
    "3d-pose",
    "championship-intelligence",
    "deep-south-sports",
    "machine-learning",
    "performance-analysis"
  ],
  "author": "Austin Humphrey <austin@blazesportsintel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahump20/BSI.git"
  },
  "homepage": "https://blazesportsintel.com",
  "bugs": {
    "url": "https://github.com/ahump20/BSI/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.9.0",
    "husky": "^9.1.7",
    "lighthouse": "^12.2.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.17.1",
    "style-dictionary": "^4.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^4.40.2"
  },
  "dependencies": {
    "@babylonjs/core": "^6.48.1",
    "@babylonjs/loaders": "^6.48.1",
    "@babylonjs/materials": "^6.48.1",
    "@prisma/client": "^6.17.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.2.0",
    "joi": "^17.10.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "winston": "^3.10.0"
  }
}
